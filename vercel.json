{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/install.html",
      "dest": "/install.html"
    },
    {
      "src": "/telegram-auth.html",
      "dest": "/telegram-auth.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://tuktuk-server.onrender.com/api",
    "REACT_APP_SOCKET_URL": "https://tuktuk-server.onrender.com"
  }
}
